  </object>

  <function name="CRETIN_FCN" type="boolean">
    VERB = GLOBAL.PRSA
    if (VERB_P(V_TELL)) {
      GLOBAL.P_CONT = false
      GLOBAL.QUOTE_FLAG = false
      msg ("Talking to yourself is said to be a sign of impending mental collapse.")
      return (true)
    }
    else if (VERB_P(V_GIVE) and GLOBAL.PRSI = ME) {
      PERFORM (V_TAKE, GLOBAL.PRSO, NOTHING)
      return (true)
    }
    else if (VERB_P(V_MAKE)) {
      msg ("Only you can do that.")
      return (true)
    }
    else if (VERB_P(V_DISEMBARK)) {
      msg ("You'll have to do that on your own.")
      return (true)
    }
    else if (VERB_P(V_EAT)) {
      msg ("Auto-cannibalism is not the answer.")
      return (true)
    }
    else if (VERB_P(V_ATTACK)) {
      if (not GLOBAL.PRSI = NOTHING) {
        if (GLOBAL.PRSI.WEAPONBIT) {
          JIGS_UP ("If you insist.... Poof, you're dead!")
          return (true)
        }
      }
      msg ("Suicide is not the answer.")
      return (true)
    }
    else if (VERB_P(V_THROW)) {
      if (GLOBAL.PRSO = ME) {
        msg ("Why don't you just walk like normal people?")
        return (true)
      }
    }
    else if (VERB_P(V_TAKE)) {
      msg ("How romantic!")
      return (true)
    }
    else if (VERB_P(V_EXAMINE)) {
      if (GLOBAL.HERE = LOC(MIRROR_1) or GLOBAL.HERE = LOC(MIRROR_2)) {
        msg ("Your image in the mirror looks tired.")
      }
      else {
        msg ("That's difficult unless your eyes are prehensile.")
      }
      return (true)
    }
    return (false)
  </function>

