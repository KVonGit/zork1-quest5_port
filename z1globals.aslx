  <function name="SAILOR_FCN" type="boolean">
    if (VERB_P(V_TELL)) {
      GLOBAL.P_CONT = false
      GLOBAL.QUOTE_FLAG = false
      msg ("You can't talk to the sailor that way.")
      return (true)
    }
    else if (VERB_P(V_EXAMINE)) {
      msg ("There is no sailor to be seen.")
      return (true)
    }
    else if (VERB_P(V_HELLO)) {
      GLOBAL.HS = GLOBAL.HS + 1
      if (GLOBAL.HS % 20 = 0) {
        msg ("You seem to be repeating yourself.")
      }
      else if (GLOBAL.HS % 10 = 0) {
        msg ("I think that phrase is getting a bit worn out.")
      }
      else {
        msg ("Nothing happens here.")
      }
      return (true)
    }
    return (false)
  </function>

