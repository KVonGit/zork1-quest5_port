  <function name="GRUE_FUNCTION" type="boolean">
    if (VERB_P(V_EXAMINE)) {
      msg ("The grue is a sinister, lurking presence in the dark places of the earth. Its favorite diet is adventurers, but its insatiable appetite is tempered by its fear of light. No grue has ever been seen by the light of day, and few have survived its fearsome jaws to tell the tale.")
      return (true)
    }
    else if (VERB_P(V_FIND)) {
      msg ("There is no grue here, but I'm sure there is at least one lurking in the darkness nearby. I wouldn't let my light go out if I were you!")
      return (true)
    }
    else if (VERB_P(V_LISTEN)) {
      msg ("It makes no sound but is always lurking in the darkness nearby.")
      return (true)
    }
    return (false)
  </function>

