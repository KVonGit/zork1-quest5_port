  <object name="NOT_HERE_OBJECT">
    <inherit name="editor_object" />
    <alias>such thing</alias>
    <ACTION>NOT_HERE_OBJECT_F</ACTION>
  </object>

  <function name="NOT_HERE_OBJECT_F" type="boolean">
    if (GLOBAL.PRSO = NOT_HERE_OBJECT and GLOBAL.PRSI = NOT_HERE_OBJECT) {
      msg ("Those things aren't here!")
    }
    else if (GLOBAL.PRSO = NOT_HERE_OBJECT) {
      TBL = GLOBAL.P_PRSO
    }
    else {
      TBL = GLOBAL.P_PRSI
      PRSO_P = false
    }
    GLOBAL.P_CONT = false
    GLOBAL.QUOTE_FLAG = false
    if (GLOBAL.WINNER = GLOBAL.PLAYER) {
      TELL ("You can't see any ")
      NOT_HERE_PRINT (PRSO_P)
      msg ("here!")
    }
    else {
      TELL ("The " + GLOBAL.WINNER.alias + " seems confused. \"I don't see any ")
      NOT_HERE_PRINT (PRSO_P)
      msg ("here!\"")
    }
    return (true)
  </function>

  <function name="NOT_HERE_PRINT" parameters="PRSO_P">
    if (GLOBAL.P_OFLAG) {
      if (GLOBAL.P_XADJ) {
        msg (GLOBAL.P_XADJN)
      }
      if (GLOBAL.P_XNAM) {
        msg (GLOBAL.P_XNAM)
      }
    }
    else if (PRSO_P) {
      msg (GET(GLOBAL.P_ITBL, GLOBAL.P_NC1) + GET(GLOBAL.P_ITBLL, GLOBAL.P_NC2L))
    }
  </function>
  
  <function name="NULL_F" parameters="A1, A2" type="boolean">
    if (not IsDefined("A1")) {
      A1 = false
    }
    if (not IsDefined("A2")) {
      A2 = false
    }
    return (false)
  </function>

