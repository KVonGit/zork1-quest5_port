  <command name="V_RANDOM">
    <pattern type="string"><![CDATA[^#random (?<text>.*)$]]></pattern>
    <script>
      SuppressTurnscripts
      if (IsInt(text)) {
        n = ToInt(text)
        n = abs(n)
        RANDOM (0 - n)
      }
      else {
        msg ("Illegal call to #RANDOM.")
      }
    </script>
    <META />
    <SYNONYM>#RANDOM</SYNONYM>
    <SYNTAX>
      SCORE
    </SYNTAX>
