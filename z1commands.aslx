  <command name="V_SWING">
    <pattern type="string"><![CDATA[^(SWING|THRUST) (?<object1>.*) AT (?<object2>.*)$|^(SWING|THRUST)( (?<object1>.*))?$]]></pattern>
    <attr name="FIND_PRSO">WEAPONBIT</attr>
    <attr name="SCOPE_PRSO" type="string">HELD CARRIED HAVE</attr>
