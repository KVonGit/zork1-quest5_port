    <attr name="SCOPE_PRSI">ON_GROUND IN_ROOM</attr>
    <scope>object1=HELD;CARRIED;HAVE|object2=ON_GROUND;IN_ROOM</scope>
    <script>
      if (GLOBAL.PRSO = NOTHING){
